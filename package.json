{
  "name": "express-jsforce-playground",
  "version": "1.0.1",
  "description": "A minimal express server starter with TypeScript",
  "main": "server.js",
  "scripts": {
    "start": "node ./build/server.js",
    "start:dev": "nodemon",
    "build": "rm -rf ./build && tsc",
    "clean": "rm -rf ./build && rm -rf ./node_modules && rm -rf ./coverage && yarn cache clean && yarn",
    "clean:dev": "yarn clean && yarn start:dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage && open coverage/lcov-report/index.html",
    "tslint": "tslint '*.ts'",
    "lint-staged": "lint-staged",
    "semantic-release": "semantic-release",
    "preinstall": "node scripts/preinstall-script.js",
    "postinstall": "node scripts/postinstall-script.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thousight/Express-JSforce-Playground.git"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "node"
  ],
  "author": "thousight",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thousight/Express-JSforce-Playground/issues"
  },
  "homepage": "https://github.com/thousight/Express-JSforce-Playground#readme",
  "dependencies": {
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/graphql": "^14.0.7",
    "@types/morgan": "^1.7.35",
    "@types/winston": "^2.4.4",
    "apollo-server-express": "^2.4.8",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "jsforce": "^1.9.1",
    "moment": "^2.23.0",
    "morgan": "^1.9.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/npm": "^7.0.4",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@types/jest": "^24.0.0",
    "conventional-changelog-express": "^2.0.1",
    "husky": "^2.0.0",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "semantic-release": "^15.13.3",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2"
  }
}
