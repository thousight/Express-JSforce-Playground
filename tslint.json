{
  "extends": ["tslint-config-prettier", "tslint:recommended"],
  "rules": {
    "no-console": false,
    "semicolon": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single"],
    "arrow-parens": [true, "ban-single-arg-parens"]
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
